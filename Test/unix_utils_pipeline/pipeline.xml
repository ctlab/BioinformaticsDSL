<?xml version='1.0'?>
<pipeline name="sort_and_head">
    <import package="base" what="sort, head, find"/>

    <input name="input_file" type="file" default="a.txt"/>
    <output name="output_file" type="file" default="head.out"/>

    <step name="head">
        <head>
            <arg name="input_file" ref="input_file"/>
            <arg name="output_file" ref="output_file"/>
        </head>
    </step>
  
</pipeline>