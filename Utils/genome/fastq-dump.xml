<?xml version='1.0'?>
<pipeline name="fastq-dump">
    <input name="reads" type="file"/>
    <option name="split-files" repr="--split-files " type="void" default="True"/>
    <output name="fastq_reads1" type="file"/>
    <output name="fastq_reads2" type="file"/>
    <shell cmd="fastq-dump" fmt="cmd [options]... reads"/>
</pipeline>